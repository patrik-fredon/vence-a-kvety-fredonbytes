# Development Environment Variables
# Copy this file to .env.local and update with your actual values

# Application
NEXT_PUBLIC_BASE_URL=http://localhost:3000
NEXT_PUBLIC_API_URL=/api
NODE_ENV=development

# Database (Supabase) - Development values
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Authentication (NextAuth)
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=your_nextauth_secret_for_development

# Payment providers - Test keys
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_test_key
STRIPE_SECRET_KEY=sk_test_your_stripe_test_secret
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_test_webhook_secret
GOPAY_CLIENT_ID=your_test_gopay_client_id
GOPAY_CLIENT_SECRET=your_test_gopay_client_secret

# Redis cache (Upstash Redis) - Development
REDIS_URL=your_redis_url
REDIS_TOKEN=your_redis_token

# Email service (Resend) - Development
RESEND_API_KEY=re_your_development_resend_api_key
RESEND_FROM_EMAIL=noreply@localhost
RESEND_REPLY_TO=info@localhost
ADMIN_EMAIL=admin@localhost

# Security
CSRF_SECRET=your_csrf_secret_for_development
ENCRYPTION_KEY=your_encryption_key_for_development

# Rate limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_REQUESTS_PER_HOUR=2000

# Performance monitoring
ENABLE_PERFORMANCE_MONITORING=false
PERFORMANCE_SAMPLE_RATE=1.0
